  ],
  "main": "build/ske-viz.min.js",
  "scripts": {
    "build": "webpack --env build",
    "start": "webpack-dev-server --config webpack.config.js --inline --colors --env dev --port 8080",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint:fix": "./node_modules/.bin/eslint ./src --fix",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "author": {
    "name": "Lucia Kocincova",
    "email": "ping@lucyia.com",
    "url": "lucyia.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucyia/ske-viz.git"
  },
  "dependencies": {
    "d3-array": "^2.0.3",
    "d3-color": "^1.2.3",
    "d3-ease": "^1.0.5",
    "d3-force": "^2.0.1",
    "d3-request": "^1.0.6",
    "d3-scale": "^3.0.0",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.5",
    "d3-transition": "^1.2.0",
    "kld-intersections": "^0.4.3",
    "lodash": "^4.17.11"
