  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "mocha": "^10.2.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": ">=3.1.11",
    "yargs": "^13.2.2"
  },
  "license": "BSD-3-Clause"
